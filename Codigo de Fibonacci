
inicio = int(input("Digite um numero para come√ßar a sequencia: "))
quantidade = int(input("\nDigite uma quantidade de termos: "))

def fibonacci(n):
    if n == 1 or n == 2:
        return 1
    else:
        return fibonacci(n-1) + fibonacci(n-2)
        
for i in range (inicio, inicio+quantidade):
      print(fibonacci(i))
